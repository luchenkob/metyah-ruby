console.log('<%= "#{@voter.id} #{@action} #{@votable.id}" %>');

var klass = '#votes-<%= @votable.id %>'
var query = $(klass);
if (query.length > 0) {
  query.html("<%= j render(partial: "user/voting/votes", locals: {voter: @voter, votable: @votable, display_status: false }) %>");
} else {
  console.log('#' + klass + ' not found');
}